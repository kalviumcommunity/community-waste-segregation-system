model User {
id String @id @default(uuid())
name String
email String @unique
role Role @default(USER)
createdAt DateTime @default(now())
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  role      Role     @default(CITIZEN)
  createdAt DateTime @default(now())
}

enum Role {
  CITIZEN
  COLLECTOR
  ADMIN
}

enum Role {
USER
COLLECTOR
ADMIN
}
